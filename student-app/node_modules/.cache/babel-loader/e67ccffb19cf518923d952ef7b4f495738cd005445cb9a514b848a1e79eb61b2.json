{"ast":null,"code":"import axios from 'axios';\nimport AIRecommendation from './AIRecommendation.vue';\nexport default {\n  name: \"CourseRecommendation\",\n  components: {\n    AIRecommendation\n  },\n  data() {\n    return {\n      courses: []\n    };\n  },\n  mounted() {\n    // 获取课程列表（实际接口根据需求调整）\n    axios.get('http://localhost:5000/api/courses').then(res => {\n      this.courses = res.data;\n    }).catch(err => console.error(err));\n  }\n};","map":{"version":3,"names":["axios","AIRecommendation","name","components","data","courses","mounted","get","then","res","catch","err","console","error"],"sources":["D:\\Code\\coding\\edu-platform\\student-app\\src\\components\\CourseRecommendation.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-container\">\r\n    <el-card class=\"page-card\">\r\n      <h2>个性化课程推荐</h2>\r\n      <!-- 调用 AIRecommendation 组件以获取个性化推荐 -->\r\n      <AIRecommendation />\r\n      <!-- 课程列表展示，网格布局 -->\r\n      <div class=\"courses-grid\">\r\n        <el-card v-for=\"course in courses\" :key=\"course.id\" class=\"course-card\" shadow=\"hover\">\r\n          <img :src=\"course.thumbnail || 'https://via.placeholder.com/300x150'\" alt=\"课程图片\" class=\"course-thumbnail\" />\r\n          <div class=\"course-info\">\r\n            <h3>{{ course.title }}</h3>\r\n            <p>{{ course.description }}</p>\r\n            <el-tag type=\"info\">{{ course.subject }}</el-tag>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport AIRecommendation from './AIRecommendation.vue'\r\nexport default {\r\n  name: \"CourseRecommendation\",\r\n  components: { AIRecommendation },\r\n  data() {\r\n    return {\r\n      courses: []\r\n    }\r\n  },\r\n  mounted() {\r\n    // 获取课程列表（实际接口根据需求调整）\r\n    axios.get('http://localhost:5000/api/courses')\r\n        .then(res => { this.courses = res.data })\r\n        .catch(err => console.error(err))\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.page-container {\r\n  padding: 20px;\r\n}\r\n.page-card {\r\n  padding: 20px;\r\n}\r\n.courses-grid {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  margin-top: 20px;\r\n}\r\n.course-card {\r\n  width: calc(25% - 20px);\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  transition: transform 0.3s;\r\n}\r\n.course-card:hover {\r\n  transform: scale(1.05);\r\n}\r\n.course-thumbnail {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n.course-info {\r\n  padding: 10px;\r\n}\r\n.course-info h3 {\r\n  margin: 0;\r\n  font-size: 18px;\r\n}\r\n.course-info p {\r\n  font-size: 14px;\r\n  color: #666;\r\n}\r\n@media (max-width: 1200px) {\r\n  .course-card { width: calc(33.33% - 20px); }\r\n}\r\n@media (max-width: 768px) {\r\n  .course-card { width: calc(50% - 20px); }\r\n}\r\n@media (max-width: 480px) {\r\n  .course-card { width: 100%; }\r\n}\r\n</style>\r\n"],"mappings":"AAsBA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,gBAAe,MAAO,wBAAuB;AACpD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,UAAU,EAAE;IAAEF;EAAiB,CAAC;EAChCG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACAN,KAAK,CAACO,GAAG,CAAC,mCAAmC,EACxCC,IAAI,CAACC,GAAE,IAAK;MAAE,IAAI,CAACJ,OAAM,GAAII,GAAG,CAACL,IAAG;IAAE,CAAC,EACvCM,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,KAAK,CAACF,GAAG,CAAC;EACtC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}