{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      profile: null\n    };\n  },\n  mounted() {\n    const token = localStorage.getItem('token');\n    axios.get('http://localhost:5000/api/student/profile', {\n      headers: {\n        'x-auth-token': token\n      }\n    }).then(res => {\n      this.profile = res.data;\n    }).catch(err => {\n      console.error(err);\n    });\n  }\n};","map":{"version":3,"names":["axios","data","profile","mounted","token","localStorage","getItem","get","headers","then","res","catch","err","console","error"],"sources":["D:\\Code\\coding\\edu-platform\\student-app\\src\\components\\Profile.vue"],"sourcesContent":["<template>\r\n  <div class=\"profile-container\" v-if=\"profile\">\r\n    <el-card class=\"profile-card\">\r\n      <h2 style=\"text-align:center;\">个人信息</h2>\r\n      <el-descriptions title=\"详细信息\" :column=\"1\" style=\"margin-top: 20px;\">\r\n        <el-descriptions-item label=\"姓名\">\r\n          <i class=\"fas fa-user\" style=\"margin-right:5px;\"></i>{{ profile.name }}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item label=\"邮箱\">\r\n          <i class=\"fas fa-envelope\" style=\"margin-right:5px;\"></i>{{ profile.email }}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item label=\"年龄\">\r\n          <i class=\"fas fa-birthday-cake\" style=\"margin-right:5px;\"></i>{{ profile.profile.age }}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item label=\"年级\">\r\n          <i class=\"fas fa-school\" style=\"margin-right:5px;\"></i>{{ profile.profile.grade }}\r\n        </el-descriptions-item>\r\n        <el-descriptions-item label=\"学习需求\">\r\n          <i class=\"fas fa-comment-dots\" style=\"margin-right:5px;\"></i>{{ profile.profile.needs }}\r\n        </el-descriptions-item>\r\n      </el-descriptions>\r\n    </el-card>\r\n  </div>\r\n  <div v-else class=\"loading-container\">\r\n    <el-skeleton animated />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data() {\r\n    return {\r\n      profile: null\r\n    }\r\n  },\r\n  mounted() {\r\n    const token = localStorage.getItem('token')\r\n    axios.get('http://localhost:5000/api/student/profile', {\r\n      headers: { 'x-auth-token': token }\r\n    }).then(res => {\r\n      this.profile = res.data\r\n    }).catch(err => {\r\n      console.error(err)\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.profile-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding: 40px 20px;\r\n}\r\n.profile-card {\r\n  width: 100%;\r\n  max-width: 500px;\r\n  border-radius: 8px;\r\n  padding: 20px;\r\n}\r\n.loading-container {\r\n  text-align: center;\r\n  padding: 40px;\r\n}\r\n</style>\r\n"],"mappings":"AA6BA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO;IAC1CN,KAAK,CAACO,GAAG,CAAC,2CAA2C,EAAE;MACrDC,OAAO,EAAE;QAAE,cAAc,EAAEJ;MAAM;IACnC,CAAC,CAAC,CAACK,IAAI,CAACC,GAAE,IAAK;MACb,IAAI,CAACR,OAAM,GAAIQ,GAAG,CAACT,IAAG;IACxB,CAAC,CAAC,CAACU,KAAK,CAACC,GAAE,IAAK;MACdC,OAAO,CAACC,KAAK,CAACF,GAAG;IACnB,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}