     </el-alert>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        subject: '',\r\n        need: ''\r\n      },\r\n      response: null\r\n    }\r\n  },\r\n  methods: {\r\n    async onSubmit() {\r\n      const token = localStorage.getItem('token')\r\n      try {\r\n        const res = await axios.post('http://localhost:5000/api/student/request-teacher', this.form, {\r\n          headers: { 'x-auth-token': token }\r\n        })\r\n        this.response = res.data\r\n      } catch (err) {\r\n        this.$message.error('请求失败')\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.form-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding: 40px 20px;\r\n}\r\n.form-card {\r\n  width: 100%;\r\n  max-width: 500px;\r\n  border-radius: 8px;\r\n  padding: 20px;\r\n}\r\n.response-container {\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,OAAO,EAAE,EAAE;QACXC,IAAI,EAAE;MACR,CAAC;MACDC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,OAAO;MAC1C,IAAI;QACF,MAAMC,GAAE,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAC,mDAAmD,EAAE,IAAI,CAACV,IAAI,EAAE;UAC3FW,OAAO,EAAE;YAAE,cAAc,EAAEL;UAAM;QACnC,CAAC;QACD,IAAI,CAACH,QAAO,GAAIM,GAAG,CAACV,IAAG;MACzB,EAAE,OAAOa,GAAG,EAAE;QACZ,IAAI,CAACC,QAAQ,CAACC,KAAK,CAAC,MAAM;MAC5B;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}